{% extends "layout.njk" %}

{% block title %}
  {{ title }}
{% endblock %}

{% block main %}
  <h1>{{ title }}</h1>
  <ul>
    {% for author in authorList %}
      <li>
        <a href="{{ author.url }}">{{ author.name }}</a>
      </li>
    {% else %}
      <li>No authors listed.</li>
    {% endfor %}
  </ul>
{% endblock %}